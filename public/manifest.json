{
  "manifest_version": 3,
  "name": "Yew Tab",
  "version": "0.2.0",
  "description": "用卡片方式展示 Edge 收藏夹。",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["bookmarks", "storage"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
